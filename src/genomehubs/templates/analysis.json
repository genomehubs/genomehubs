      "assembly_count": {
        "type": "short",
        "meta": { "description": "Count of associated assembly_ids" }
      },
      "taxon_count": {
        "type": "short",
        "meta": { "description": "Count of associated taxon_ids" }
      },
      "file_count": {
        "type": "short",
        "meta": { "description": "Count of associated file_ids" }
      },
      "feature_count": {
        "type": "short",
        "meta": { "description": "Count of associated feature_ids" }
