{
  "name": "genomehubs-ui",
  "version": "2.12.4",
  "description": "Frontend for GenomeHubs",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --mode production --config ./webpack.config.js",
    "build:analyze": "ANALYZE=true NODE_ENV=production webpack --mode production --config ./webpack.config.js",
    "client": "NODE_OPTIONS='--max-http-header-size=100000 --trace-deprecation' webpack serve --mode development",
    "start": "node src/server/index.js",
    "start:ssr": "SSR_MODE=all node src/server/index.js",
    "start:bots": "SSR_MODE=bots node src/server/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/client --ext .jsx,.js",
    "lint:fix": "eslint src/client --ext .jsx,.js --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic",
    "docker:build": "docker build -f ../docker/ui/Dockerfile -t genomehubs-ui:latest ../..",
    "docker:build:version": "docker build -f ../docker/ui/Dockerfile -t genomehubs-ui:$npm_package_version ../..",
    "docker:run": "docker run --rm -p 8880:8880 genomehubs-ui:latest",
    "docker:run:ssr": "docker run --rm -p 8880:8880 -e SSR_MODE=all genomehubs-ui:latest",
    "docker:run:bots": "docker run --rm -p 8880:8880 -e SSR_MODE=bots genomehubs-ui:latest",
    "docker:run:content": "docker run --rm -p 8880:8880 -v /path/to/content:/content genomehubs-ui:latest",
    "docker:run:envfile": "docker run --rm -p 8880:8880 --env-file ../docker/ui/.env genomehubs-ui:latest",
    "docker:stop": "docker ps -q --filter ancestor=genomehubs-ui:latest | xargs -r docker stop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genomehubs/goat-ui.git"
  },
  "author": "Richard Challis",
  "license": "MIT",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "bugs": {
    "url": "https://github.com/genomehubs/goat-ui/issues"
  },
