{
  "extends": ["plugin:prettier/recommended", "plugin:storybook/recommended"],
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "React": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "plugins": ["react"],
  "parser": "babel-eslint",
  "rules": {
    "no-undef": "error"
  }
}
